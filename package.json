{
  "name": "boilerplate-react-typescript-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --mode=production --config=config/client/webpack.prod.js --node-env=production",
    "build:server": "webpack --mode=production --config=config/server/webpack.prod.js --node-env=production",
    "dev": "npm run dev:server && npm run dev:client",
    "dev:client": "webpack --mode=development --config=config/client/webpack.dev.js",
    "dev:server": "webpack --mode=development --config=config/server/webpack.dev.js",
    "docker:build": "docker build . -t boilerplate-react-typescript-nodejs",
    "docker:run": "docker run --name boilerplate-react-typescript-nodejs --rm -p 8080:8080 -it boilerplate-react-typescript-nodejs",
    "start": "node ./dist/server/index.js",
    "lint": "node ./scripts/lint/default.js && node ./scripts/lint/prettify.js",
    "lint:deep": "node ./scripts/lint/deep.js && node ./scripts/lint/prettify.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "node ./scripts/hooks/install.js",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "license": "MIT",
  "private": true,
  "author": "jpiggg <isdolinskayaym@gmail.com>",
